{
  "name": "only1",
  "scripts": {
    "dev": "next dev",
    "dev:w": "start \"\" http://localhost:3000 && next dev",
    "dev:m": "open http://localhost:3000 && next dev",
    "build": "next build",
    "start": "next start",
    "type-check": "tsc --project tsconfig.json --pretty --noEmit",
    "lint": "eslint --ext js,jsx,ts,tsx --fix",
    "postinstall": "husky install",
    "test": "jest",
    "deploy": "git pull origin main && git checkout main && git pull origin main && git merge dev && git push && git checkout dev && git pull origin dev && git merge main && git checkout dev && git push",
    "stash": "git stash save -m echo `date`",
    "stash:list": "git stash list --format='%gd (%cr): %gs'",
    "stash:update-ref": "git update-ref refs/stash stash@{0}",
    "config:post-buffer": "git config --global http.postBuffer 2048M",
    "config:request-buffer": "git config --global http.maxRequestBuffer 1024M",
    "config:compression": "git config --global core.compression 9",
    "webp": "for file in *; do cwebp -q 100 $file -o ${file%.*}.webp; done",
    "remove-files": "for file in *.webp; do rm $file; done"
  },
  "dependencies": {
    "@emotion/css": "^11.7.1",
    "@emotion/react": "^11.4.1",
    "@emotion/styled": "^11.3.0",
    "@mui/material": "^5.0.2",
    "@mui/styles": "^5.0.1",
    "framer-motion": "^4.1.17",
    "next": "^11.0.1",
    "next-useragent": "^2.8.0",
    "react": "^17.0.1",
    "react-bootstrap": "^1.6.1",
    "react-dom": "^17.0.1",
    "recharts": "^2.1.4"
  },
  "devDependencies": {
    "@babel/core": "^7.14.6",
    "@emotion/babel-plugin": "^11.7.2",
    "@emotion/server": "^11.4.0",
    "@types/jest": "^26.0.24",
    "@types/node": "^14.17.5",
    "@types/react": "^17.0.14",
    "@types/react-dom": "^17.0.9",
    "@types/react-test-renderer": "^17.0.1",
    "@typescript-eslint/eslint-plugin": "^4.28.2",
    "@typescript-eslint/parser": "^4.28.2",
    "babel-jest": "^26.6.3",
    "eslint": "^7.30.0",
    "eslint-config-next": "^11.0.1",
    "eslint-config-prettier": "^8.3.0",
    "eslint-plugin-jsx-a11y": "^6.4.1",
    "eslint-plugin-prettier": "^3.4.0",
    "eslint-plugin-react": "^7.24.0",
    "eslint-plugin-react-hooks": "^4.2.0",
    "husky": "^5.1.3",
    "identity-obj-proxy": "^3.0.0",
    "jest": "^26.6.3",
    "jest-watch-typeahead": "^0.6.4",
    "lint-staged": "^10.5.4",
    "prettier": "^2.3.2",
    "react-test-renderer": "^17.0.1",
    "sass": "^1.32.8",
    "typescript": "^4.3.5"
  },
  "version": "1.0.0",
